top_srcdir := @top_srcdir@
srcdir = @srcdir@
topdir = ../..
subdir := ir/common

INSTALL_HEADERS := bool.h common.h firm.h

SOURCES = $(INSTALL_HEADERS)

SOURCES += Makefile.in panic.c tune.h xgprintf.c xp_help.h common.c firm.c \
		misc.h panic.h xfprintf.c xoprintf.c xprintf.h common_t.h

include $(topdir)/MakeRules

CPPFLAGS += -I$(top_srcdir)/ir/common -I$(top_srcdir)/ir/ident -I$(top_srcdir)/ir/tr \
		-I$(top_srcdir)/ir/adt -I$(top_srcdir)/ir/tv -I$(top_srcdir)/ir/ir \
		-I$(top_srcdir)/ir/ana -I$(top_srcdir)/ir/st

include $(top_srcdir)/MakeTargets

all: subdir.o
