top_srcdir := @top_srcdir@
srcdir = @srcdir@
topdir = ../..
subdir := ir/ana

INSTALL_HEADERS = irouts.h irdom.h cgana.h irloop.h irtypeinfo.h irsimpletype.h rta.h

SOURCES = $(INSTALL_HEADERS)

SOURCES += Makefile.in \
		irouts.c irdom_t.h irdom.c cgana.c	rta.c \
		irloop_t.h irbackedge.c irbackedge_t.h irscc.c ircfscc.c irtypeinfo.c irsimpletype.c

include $(topdir)/MakeRules

CPPFLAGS += -I$(top_srcdir)/ir/adt -I$(top_srcdir)/ir/ir -I$(top_srcdir)/ir/common  \
		-I$(top_srcdir)/ir/ident -I$(top_srcdir)/ir/tr -I$(top_srcdir)/ir/tv \
		-I$(top_srcdir)/ir/debug -I$(top_srcdir)/ir/ana -I$(top_srcdir)/ir/st

include $(top_srcdir)/MakeTargets

all: subdir.o
